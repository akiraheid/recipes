html
	head
		title Log in
	body
		h1 Log in
		form(action='/login' method='post')
			div
				label(for='name') Username
				input(type='text' id='name' name='username')

			div
				label(for='pass') Password
				input(type='password' id='pass' name='password')

			div
				button(type='submit') Log in
